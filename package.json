{
  "name": "overlay",
  "version": "0.0.1",
  "description": "Minecraft Overlay",
  "main": "src/main/index.js",
  "scripts": {
    "test": "npx electron .",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "build:dir": "electron-builder --dir",
    "build:win": "electron-builder --win portable",
    "build:mac": "electron-builder --mac",
    "dist:win": "npx @electron/packager . Overlay --platform=win32 --arch=x64 --out=dist/ --overwrite --asar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlpoProo/overlay.git"
  },
  "author": "AlpoProo",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/AlpoProo/overlay/issues"
  },
  "type": "commonjs",
  "homepage": "https://github.com/AlpoProo/overlay#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "consoled.js": "^1.0.4",
    "custom-electron-titlebar": "^4.2.8"
  },
  "devDependencies": {
    "@electron/packager": "^17.1.2",
    "electron": "^34.0.1",
    "electron-builder": "^24.9.1",
    "electron-icon-builder": "^2.0.1",
    "electron-packager": "^17.1.2"
  },
  "build": {
    "appId": "com.alpoproo.overlay",
    "productName": "Bedwars Overlay",
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "forceCodeSigning": false,
    "win": {
      "target": "portable",
      "artifactName": "${productName}-${version}-portable.${ext}"
    },
    "mac": {
      "target": "dmg",
      "category": "public.app-category.games"
    },
    "portable": {
      "artifactName": "${productName}-${version}-portable.${ext}"
    }
  }
}
